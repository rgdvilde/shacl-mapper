@prefix dash: <http://datashapes.org/dash#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ngsi: <https://uri.fiware.org/ns/data-models#> .
@prefix ngsic: <https://uri.etsi.org/ngsi-ld/> .
@prefix ex: <https://example.com/> .

ngsi:BikeHireDockingStationShape
    a sh:NodeShape ;
    sh:targetClass ngsic:BikeHireDockingStation ;
    sh:property [
        sh:path ngsic:name ;
        sh:datatype xsd:string ;
    ] ;
    sh:property [
        sh:path ngsic:location ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path ngsic:source ;
        sh:minCount 1 ;
    ] ;
    
    sh:property [
        sh:path ex:dateTime ;
        sh:datatype xsd:dateTime ;
    ] .

ngsi:SomethingElseShape
    a sh:NodeShape ;
    sh:targetClass ngsic:SomeThingElse ;
    sh:property [
        sh:path ngsic:name ;
        sh:datatype xsd:string ;
    ] .